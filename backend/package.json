{
  "name": "backend",
  "version": "0.0.0",
  "description": "Backend part (Node)",
  "repository": "https://github.com/linuspahl/make-your-day",
  "author": "Linus Pahl",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "nodemon --exec babel-node --inspect ./src/index.js",
    "startProd": "pm2 start --interpreter babel-node ./src/index.js",
    "stopProd": "pm2 stop index",
    "logProd": "pm2 logs",
    "createDatabase": "babel-node ./config/setup/createDatabase.js",
    "sequelize": "sequelize --config ./config/sequelize-cli.js --models-path ./src/models",
    "migrate": "yarn sequelize db:migrate",
    "migrateDown": "yarn sequelize db:migrate:undo",
    "seed": "yarn sequelize db:seed:all",
    "lint": "eslint config migrations seeders src"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.3.1",
    "bcrypt-nodejs": "^0.0.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "graphql-resolvers": "^0.3.2",
    "jsonwebtoken": "^8.5.0",
    "moment": "^2.24.0",
    "pg": "^7.7.1",
    "pm2": "^3.3.1",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
  }
}
