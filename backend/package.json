{
  "name": "backend",
  "version": "0.0.0",
  "description": "Backend part (Node)",
  "repository": "https://github.com/linuspahl/make-your-day",
  "author": "Linus Pahl",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "nodemon --exec babel-node --inspect ./src/index.js",
    "startProd": "pm2 start --interpreter babel-node ./src/index.js",
    "stopProd": "pm2 stop index",
    "logProd": "pm2 logs",
    "createDatabase": "babel-node ./config/setup/createDatabase.js",
    "sequelize": "sequelize --config ./config/sequelize-cli.js --models-path ./src/models",
    "migrate": "yarn sequelize db:migrate",
    "migrateDown": "yarn sequelize db:migrate:undo",
    "seed": "yarn sequelize db:seed:all",
    "createMocks": "yarn sequelize db:seed:all --seeders-path mocks",
    "lint": "eslint config migrations seeders src"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.4",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "apollo-server-express": "^2.6.7",
    "bcrypt-nodejs": "^0.0.3",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "graphql-resolvers": "^0.3.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "pg": "^7.11.0",
    "pm2": "^3.5.1",
    "sequelize": "^5.9.4",
    "sequelize-cli": "^5.5.0"
  }
}
