  "scripts": {
    "start": "TS_NODE_PROJECT=\"./config/webpack/tsconfig-for-webpack-config.json\" webpack-dev-server --env.mode development --config config/webpack/common.ts",
    "build": "TS_NODE_PROJECT=\"./config/webpack/tsconfig-for-webpack-config.json\" webpack --progress --verbose --env.mode production --env.presets compress --config config/webpack/common.ts",
    "analyzeDev": "yarn start --env.presets analyze",
    "analyzeProd": "yarn build --env.presets analyze",
    "preCommit": "yarn lint && yarn checkTypes && yarn test",
    "lint": "yarn run eslint src config --debug --ext .js,.ts,.tsx",
    "checkTypes": "yarn run tsc",
    "test": "jest --config config/test/jestConfig.js --coverage"
  },
